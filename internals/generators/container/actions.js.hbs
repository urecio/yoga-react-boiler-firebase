import * as constants from './constants';

{{#if wantAsyncRequests}}
  {{#asyncRequests}}
export function {{name}}Request(payload) {
  return {
    type: constants.{{ upperCase name }}_REQUEST,
    payload,
  };
}
export function {{name}}Response(payload) {
  return {
    type: constants.{{ upperCase name }}_RESPONSE,
    payload,
  };
}
  {{/asyncRequests}}
{{else}}
export function defaultAction() {
  return {
    type: constants.DEFAULT_ACTION,
  };
}
{{/if}}
